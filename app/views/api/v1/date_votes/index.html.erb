<p id="notice"><%= notice %></p>

<h1>Date Votes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_votes.each do |date_vote| %>
      <tr>
        <td><%= link_to 'Show', date_vote %></td>
        <td><%= link_to 'Edit', edit_date_vote_path(date_vote) %></td>
        <td><%= link_to 'Destroy', date_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date Vote', new_date_vote_path %>
